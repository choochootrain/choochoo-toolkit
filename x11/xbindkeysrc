"/home/hp/bin/volume.sh toggle"
    m:0x0 + c:121
    XF86AudioMute

"/home/hp/bin/volume.sh down"
    m:0x0 + c:122
    XF86AudioLowerVolume

"/home/hp/bin/volume.sh up"
    m:0x0 + c:123
    XF86AudioRaiseVolume

"/home/hp/bin/brightness.sh down"
    m:0x0 + c:232
    XF86MonBrightnessDown

"/home/hp/bin/brightness.sh up"
    m:0x0 + c:233
    XF86MonBrightnessUp
