set homepage http://duckduckgo.com

searchengine ggl http://www.google.com/search?q=%s
searchengine wiki http://en.wikipedia.org/w/index.php?search=%s

set download-dir ~/downloads
set download-command "wget --load-cookies ~/.config/jumanji/cookies '%s' -O '%s'"

set font monospace normal 9
set inputbar_fgcolor #268bd2
set statusbar_ssl_bgcolor #268bd2
set tabbar_focus_fgcolor #268bd2
set completion_hl_bgcolor #268bd2

set auto_save 60
set default_session_name start
set session_restored true
set developer_extras true

script ~/.config/jumanji/scripts/hints.js
